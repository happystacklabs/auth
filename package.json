{
  "name": "@happystack/frontend-boilerplate",
  "version": "0.2.1",
  "private": true,
  "author": "Heinrich Tremblay",
  "homepage": "https://boilerplate.happystack.io",
  "repository": "https://github.com/happystacklabs/frontend-boilerplate",
  "bugs": {
    "url": "https://github.com/happystacklabs/frontend-boilerplate/issues"
  },
  "keywords": [
    "happystack",
    "boilerplate",
    "react",
    "redux",
    "jwt"
  ],
  "license": "MIT",
  "dependencies": {
    "@happystack/kit": "^0.3.6",
    "axios": "^0.17.1",
    "dotenv": "^4.0.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.1",
    "validator": "^9.1.2"
  },
  "scripts": {
    "start": "concurrently \"npm run watch-css\" \"npm run start-js\"",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "deploy": "runner deploy.sh",
    "lint": "eslint src/**"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "moxios": "^0.4.0",
    "nock": "^9.1.4",
    "node-sass-chokidar": "0.0.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.2.0"
  }
}
